{
	"name": "bay-lang",
	"displayName": "BayLang",
	"description": "BayLang VSCode extension",
	"publisher": "BAYRELL",
	"version": "0.1.1",
	"main": "index.js",
	"author": "Ildar Bikmamatov <ildar@bayrell.org>",
	"license": "Apache-2.0",
	"homepage": "https://github.com/bayrell",
	"extensionUri": "https://github.com/bayrell/baylang-vscode",
	"repository": {
		"type": "git",
		"url": "https://github.com/bayrell/baylang-vscode"
	},
	"activationEvents": [
		"onLanguage:bay"
	],
	"engines": {
		"vscode": "^1.70.0"
	},
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"languages": [{
			"id": "bay",
			"aliases": ["BayLang", "Bay Lang", "bay"],
			"extensions": ["bay"]
		}],
		"grammars": [{
			"language": "bay",
			"scopeName": "source.bay",
			"path": "./syntaxes/bay.tmLanguage.json"
		}]
	}
}
